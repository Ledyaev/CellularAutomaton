@{
    ViewBag.Title = "Галерея";
}

<h2>Галерея</h2>
<div class="container">
    <div class="bg-primary col-md-6 col-md-offset-3">
        <div class="navbar ">
            <form class="navbar-left navbar-form" role="search">
                <div class="row">
                    <div class="form-group">
                        <input id="criteria" class="form-control" placeholder="Search">
                    </div>
                    <div id="searchAutomatons" class="btn btn-default ">Поиск</div>
